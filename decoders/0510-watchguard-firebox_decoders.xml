<!-- FIREWALL DECODER -->
<decoder name="watchguard-firebox-firewall">
  <prematch type="pcre2">(FVE\w+|D0F\w+|D02\w+)\s(\(\S+\))?\s?firewall: </prematch>
</decoder>

