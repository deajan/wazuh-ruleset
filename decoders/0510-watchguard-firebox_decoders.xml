<decoder name="watchguard-firebox-firewall-info">
  <parent>watchguard-firebox-firewall</parent>
  <regex type="pcre2" offset="after_parent">msg_id="([0-9a-fA-F]{4}-[0-9a-fA-F]{4})"</regex>
  <order>id</order>
</decoder>

